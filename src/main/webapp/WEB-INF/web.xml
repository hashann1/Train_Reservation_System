<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
         <servlet>
         	<servlet-name>login</servlet-name>
         	<servlet-class>com.customer.LoginServlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>login</servlet-name>
         	<url-pattern>/log</url-pattern>
         </servlet-mapping>   
         
         <servlet>
         		<servlet-name>customerinsert</servlet-name>
         		<servlet-class>com.customer.CustomerInsert</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>customerinsert</servlet-name>
         	<url-pattern>/insert</url-pattern>
         </servlet-mapping>



         
          <servlet>
         		<servlet-name>updatecustomer</servlet-name>
         		<servlet-class>com.customer.UpdateCustomerServlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>updatecustomer</servlet-name>
         	<url-pattern>/update</url-pattern>
         </servlet-mapping>
         
          <servlet>
         		<servlet-name>deletecustomer</servlet-name>
         		<servlet-class>com.customer.DeleteCustomerServlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>deletecustomer</servlet-name>
         	<url-pattern>/delete</url-pattern>
         </servlet-mapping>
         
         
         
         
         
         <servlet>
         		<servlet-name>bookinginsert</servlet-name>
         		<servlet-class>com.customer.bookinginsert</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>bookinginsert</servlet-name>
         	<url-pattern>/book</url-pattern>
         </servlet-mapping>
         
         
         
          <servlet>
         		<servlet-name>showbooking</servlet-name>
         		<servlet-class>com.customer.LoginBooking</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>showbooking</servlet-name>
         	<url-pattern>/show</url-pattern>
         </servlet-mapping>
         
         
         
         <servlet>
         		<servlet-name>updatebooking</servlet-name>
         		<servlet-class>com.customer.UpdateBookingServlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>updatebooking</servlet-name>
         	<url-pattern>/updateb</url-pattern>
         </servlet-mapping>
         
          <servlet>
         		<servlet-name>deletbooking</servlet-name>
         		<servlet-class>com.customer.DeleteBookingServlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>deletbooking</servlet-name>
         	<url-pattern>/deleteb</url-pattern>
         </servlet-mapping>
         
         
         
         
         
         
          <servlet>
         		<servlet-name>paymentinsert</servlet-name>
         		<servlet-class>com.customer.PaymentInsert</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>paymentinsert</servlet-name>
         	<url-pattern>/paym</url-pattern>
         </servlet-mapping>
         
          <servlet>
         		<servlet-name>logpayment</servlet-name>
         		<servlet-class>com.customer.PaymentServlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>logpayment</servlet-name>
         	<url-pattern>/pay</url-pattern>
         </servlet-mapping>
         
         
          <servlet>
         		<servlet-name>updatepayment</servlet-name>
         		<servlet-class>com.customer.UpdatePaymentServlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>updatepayment</servlet-name>
         	<url-pattern>/updatepay</url-pattern>
         </servlet-mapping>
         
         
         <servlet>
         		<servlet-name>deletepayment</servlet-name>
         		<servlet-class>com.customer.DeletePaymentServlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>deletepayment</servlet-name>
         	<url-pattern>/deletepay</url-pattern>
         </servlet-mapping>
         
         
         
         
         
         
</web-app>